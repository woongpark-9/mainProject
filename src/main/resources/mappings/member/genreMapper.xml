<!DOCTYPE mapper

  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="genreMapper" >
	<resultMap type="genreVO" id="genreResult">
		<id property="genre_id" column="genre_id"></id>
		<result property= "genre_name" column="genre_name"/>
		<result property= "genre_email" column = "genre_email"/>	
	</resultMap>
	
	<!--  장르 중복 체크 -->
	<select id="memberGenreCheck" resultType = "int">
		SELECT COUNT(*) FROM GENRE
		WHERE email = #{email}
	</select>
	

</mapper>